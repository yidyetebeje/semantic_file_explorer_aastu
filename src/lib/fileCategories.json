{
  "categories": [
    {
      "name": "Documents",
      "extensions": [".pdf", ".doc", ".docx", ".txt", ".md", ".odt", ".rtf", ".tex", ".wpd"],
      "keywords": ["report", "invoice", "resume", "letter", "contract", "manuscript", "paper", "draft", "notes"]
    },
    {
      "name": "Images",
      "extensions": [".jpg", ".jpeg", ".png", ".gif", ".bmp", ".svg", ".heic", ".tiff", ".webp", ".ico"],
      "keywords": ["photo", "picture", "drawing", "icon", "logo", "scan", "screenshot", "artwork", "graphic"]
    },
    {
      "name": "Audio",
      "extensions": [".mp3", ".wav", ".aac", ".flac", ".ogg", ".m4a", ".wma", ".aiff"],
      "keywords": ["music", "song", "podcast", "recording", "soundtrack", "voice memo", "lecture", "narration"]
    },
    {
      "name": "Videos",
      "extensions": [".mp4", ".mkv", ".avi", ".mov", ".wmv", ".flv", ".webm", ".mpg", ".mpeg"],
      "keywords": ["movie", "film", "clip", "recording", "tutorial", "webinar", "presentation", "footage", "short"]
    },
    {
      "name": "Archives",
      "extensions": [".zip", ".rar", ".tar", ".gz", ".7z", ".bz2", ".xz", ".iso"],
      "keywords": ["backup", "compressed", "package", "bundle", "collection", "dataset", "installer"]
    },
    {
      "name": "Code",
      "extensions": [
        ".js", ".ts", ".py", ".java", ".c", ".cpp", ".cs", ".rs", ".go", ".php", ".rb", ".swift", ".kt",
        ".html", ".css", ".scss", ".less", ".json", ".xml", ".yaml", ".yml", ".sh", ".bat", ".sql"
      ],
      "keywords": ["script", "source", "program", "library", "module", "component", "stylesheet", "markup", "query", "config"]
    },
    {
      "name": "Spreadsheets",
      "extensions": [".xls", ".xlsx", ".csv", ".ods", ".numbers"],
      "keywords": ["data", "table", "chart", "budget", "report", "ledger", "analytics", "financial", "tracker"]
    },
    {
      "name": "Presentations",
      "extensions": [".ppt", ".pptx", ".odp", ".key"],
      "keywords": ["slides", "deck", "keynote", "talk", "lecture", "workshop", "meeting", "seminar"]
    },
    {
      "name": "Ebooks",
      "extensions": [".epub", ".mobi", ".azw", ".azw3"],
      "keywords": ["book", "novel", "publication", "magazine", "textbook", "manual"]
    },
    {
      "name": "Fonts",
      "extensions": [".ttf", ".otf", ".woff", ".woff2"],
      "keywords": ["typeface", "font family", "typography", "glyphs"]
    },
    {
        "name": "Executables & System",
        "extensions": [".exe", ".app", ".dmg", ".bin", ".dll", ".so", ".jar", ".msi"],
        "keywords": ["application", "program", "installer", "binary", "library", "driver", "system file"]
    },
    {
      "name": "Other",
      "extensions": [],
      "keywords": ["miscellaneous", "unknown", "temporary", "various"]
    }
  ]
}
